  "repository": {
    "type": "git",
    "url": "git+https://github.com/Whimco-Studio/Quirk.git"
  },
  "keywords": [
    "roblox",
    "roblox-ts",
    "ui",
    "components",
    "gui",
    "typescript",
    "rbxts"
  ],
  "author": "Whimco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Whimco-Studio/Quirk/issues"
  },
  "homepage": "https://github.com/Whimco-Studio/Quirk#readme",
